	Page2Frame.Visible = true
	BuiltInExecutorFrame.Visible = false
end

function ExecutorExecuteFunct()
	loadstring(BuiltInExecutorSourceFrameTextBox.Text)()
end

function ClearExecutorFunct()
	BuiltInExecutorSourceFrameTextBox.Text = "*poof!*"
	wait(1)
	BuiltInExecutorSourceFrameTextBox.Text = ""
end

function EnableModProtectionModeFunct()
	IsModProtectionModeEnabled = true
	local PlrsInSessionModProtection = game.Players:GetChildren()
	
	for i, v in pairs(PlrsInSessionModProtection) do
		if v.ClassName == "Player" and v.UserId == 74834893 or v.UserId == 941072 or v.UserId == 15319024 then
			game.Players.LocalPlayer:Kick("A moderator of BGS has joined the game and you have been kicked to prevent being caught.")
		end
	end
	game.Players.PlayerAdded:Connect(function(plr)
		if plr.UserId == 74834893 or plr.UserId == 941072 or plr.UserId == 15319024 then
			game.Players.LocalPlayer:Kick("A moderator of Bubble Gum Simulator has joined the game and you have been kicked to prevent being caught.")
		end
	end)
end

function DisableModProtectionModeFunct()
	IsModProtectionModeEnabled = false
end

function EnableStreamerModeFunct()
	IsStreamerModeCurrentlyEnabled = true
	local PlrStreamerTag = game.Players:GetChildren()

	for i, v in pairs(PlrStreamerTag) do
		v.Character.Head.CustomPlayerTag.Title.UIGradient:Destroy()
		v.Character.Head.CustomPlayerTag.Title.Text = StreamerModeSetSelfName.Text
		v.Character.Head.CustomPlayerTag.Username.Text = ("Streamer Mode Enabled. Stay safe, friend.")
	end

	local PlrStreamerLeader = game.CoreGui.RobloxGui.PlayerListMaster.OffsetFrame.PlayerScrollList.SizeOffsetFrame.ScrollingFrameContainer.ScrollingFrameClippingFrame.ScollingFrame.OffsetUndoFrame:GetChildren()

	for i, v in pairs(PlrStreamerLeader) do
		v.ChildrenFrame.NameFrame.BGFrame.OverlayFrame.PlayerName.PlayerName.Text = StreamerModeSetOthersNames.Text
	end

	game.Players.LocalPlayer.leaderstats:Destroy()
	game.Players.LocalPlayer.PlayerGui.ScreenGui.StatsFrame.Coins.Visible = false
	game.Players.LocalPlayer.PlayerGui.ScreenGui.StatsFrame.Gems.Visible = false
	game.Players.LocalPlayer.PlayerGui.ScreenGui.StatsFrame.Bubble.Visible = false
	--game.Players.LocalPlayer.PlayerGui.ScreenGui.CollectionFrame.Main.Inner.Options.Trade:Destroy()
	
	game.Players.PlayerAdded:Connect(function(plr)
		plr.Character.Head.CustomPlayerTag.Title.Text = StreamerModeSetSelfName.Text
		plr.Character.Head.CustomPlayerTag.Username.Text = ("Streamer Mode Enabled. Stay safe, friend.")
		local PlrStreamerLeader2 = game.CoreGui.RobloxGui.PlayerListMaster.OffsetFrame.PlayerScrollList.SizeOffsetFrame.ScrollingFrameContainer.ScrollingFrameClippingFrame.ScollingFrame.OffsetUndoFrame:GetChildren()

		for i, v in pairs(PlrStreamerLeader2) do
			v.ChildrenFrame.NameFrame.BGFrame.OverlayFrame.PlayerName.PlayerName.Text = StreamerModeSetOthersNames.Text
		end
	end)
end

function StartTimer()
	while wait() do
		local EPOXTime = os.time()
		local DateEPOX = os.date("Today is %A, %B %d, %Y.", EPOXTime)
		local TimeTextEPOX = os.date("It's currently %I:%M:%S %p, %Z", EPOXTime)
		UserTimeDate.Text = (DateEPOX)
		UserTimeTime.Text = (TimeTextEPOX)
	end
end

function OpenClaimRewardMenuFunct()
	Page2Frame.Visible = false
	ClaimRewardsAutomaticallyFrame.Visible = true
end

function CloseClaimRewardMenuFunct()
	ClaimRewardsAutomaticallyFrame.Visible = false
	Page2Frame.Visible = true
end

function TeleportToMerchantFunct()
	game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-88.4751816, 45.6000061, -228.067963, 0.998906493, 0, -0.0467526503, 0, 1, 0, 0.0467526503, 0, 0.998906493)
end

function BuyMerchantItem1Funct()
	game.ReplicatedStorage.NetworkRemoteEvent:FireServer("BuyMerchantItem", 1)
end

function BuyMerchantItem2Funct()
	game.ReplicatedStorage.NetworkRemoteEvent:FireServer("BuyMerchantItem", 2)
end

function BuyMerchantItem3Funct()
	game.ReplicatedStorage.NetworkRemoteEvent:FireServer("BuyMerchantItem", 3)
end

function TeleportToDailyRewardFunct()
	game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(6.289361, 45.6000061, 6.88707972, -0.898855805, 0, 0.438244492, 0, 1, 0, -0.438244492, 0, -0.898855805)
end

function ClaimDailyRewardFunct()
	game.ReplicatedStorage.NetworkRemoteEvent:FireServer("ClaimDailyReward")
end

function EnableAutoClaimDailyRewardFunct()
	IsAutoClaimDailyRewardEnabled = true
	while wait() do
		if IsAutoClaimDailyRewardEnabled == true then
			game.ReplicatedStorage.NetworkRemoteEvent:FireServer("ClaimDailyReward")
		end
	end
end

function DisableAutoClaimDailyRewardFunct()
	IsAutoClaimDailyRewardEnabled = false
end

function TeleportToSpinTheWheelFunct()
	game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(8.19312191, 45.6000061, 47.4579201, 0.20472087, 2.94468396e-08, 0.978820384, 3.80400351e-08, 1, -3.80401026e-08, -0.978820384, 4.50219666e-08, 0.20472087)
end

function SpinTheWheelFunct()
	game.ReplicatedStorage.NetworkRemoteEvent:FireServer("SpinToWin")
end

function EnableAutoSpinTheWheelFunct()
	IsAutoSpinTheWheelEnabled = true
	while wait() do
		if IsAutoSpinTheWheelEnabled == true then
			game.ReplicatedStorage.NetworkRemoteEvent:FireServer("SpinToWin")
		end
	end
end

function DisableAutoSpinTheWheelFunct()
	IsAutoSpinTheWheelEnabled = false
end

function TeleportToShardShopFunct()
	game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-1791.55859, 1855.46985, 11071.9307, 0.309089333, 0, 0.951032996, 0, 1, 0, -0.951032996, 0, 0.309089333)
end

function TeleporttoShardRewardsFunct()
	game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-1724.65881, 1855.51233, 11034.9238, 0.982588172, 3.2758347e-37, -0.185796961, -1.92171672e-37, 1, 7.46825312e-37, 0.185796961, -6.98116806e-37, 0.982588172)
end

function ClaimShardRewardsFunct()
	game.ReplicatedStorage.NetworkRemoteEvent:FireServer("ClaimShardQuestReward")
end

function EnableAutoShardRewardsFunct()
	IsShardRewardsAutoClaimRewardsEnabled = true
	while wait() do
		if IsShardRewardsAutoClaimRewardsEnabled == true then
			game.ReplicatedStorage.NetworkRemoteEvent:FireServer("ClaimShardQuestReward")
			game.ReplicatedStorage.NetworkRemoteEvent:FireServer("GetShardQuest", SetShardAutoDifficultyAutoAssign.Text)
		end
	end
end

function DisableAutoShardRewardsFunct()
	IsShardRewardsAutoClaimRewardsEnabled = false
end

function TeleportToPotionBrewFunct()
	game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-1740.39026, 2705.99048, 11045.3525, -0.964207292, -9.08030768e-17, 0.265149534, -9.52319871e-17, 1, -3.8480256e-18, -0.265149534, -2.89610127e-17, -0.964207292)
end

function StartUltraCounter()
	UltraEggHatchValue = 1
	
	local EggsFired = Instance.new("ScreenGui")
	local SummaryAmount = Instance.new("TextLabel")
	local EggsOpened = Instance.new("TextLabel")
	local EggAmount = 0

	--Parents

	EggsFired.Parent = game.CoreGui
	SummaryAmount.Parent = EggsFired
	EggsOpened.Parent = EggsFired


	--ResetOnRespawn

	EggsFired.ResetOnSpawn = false


	--Active

	SummaryAmount.Active = true
	EggsOpened.Active = true

	--Name

	EggsFired.Name = ("EggsFired")
	SummaryAmount.Name = ("SummaryAmount")
	EggsOpened.Name = ("EggsOpened")

	--BackgroundTransparency

	SummaryAmount.BackgroundTransparency = (1)
	EggsOpened.BackgroundTransparency = (1)

	--Selectable

	EggsOpened.Selectable = false
	SummaryAmount.Selectable = false

	--Visible

	EggsOpened.Visible = true
	SummaryAmount.Visible = true

	--Fonts

	EggsOpened.Font = ("PatrickHand")
	SummaryAmount.Font = ("PatrickHand")

	--Text
	SummaryAmount.Text = ("You have opened the following amount of eggs:")

	--TextColor
	EggsOpened.TextColor3 = Color3.fromRGB(255, 0, 0)
	SummaryAmount.TextColor3 = Color3.fromRGB(255, 0, 0)

	--TextScaled
	SummaryAmount.TextScaled = false
	EggsOpened.TextScaled = true

	--TextSize
	EggsOpened.TextSize = (16)
	SummaryAmount.TextSize = (16)

	--Text Label Positions
	EggsOpened.Position = UDim2.new(0.705, 0, 0.805, 0)
	SummaryAmount.Position = UDim2.new(0.779, 0, 0.745, 0)

	--Text Label Size
	EggsOpened.Size = UDim2.new(0, 356, 0, 37)
	SummaryAmount.Size = UDim2.new(0, 166, 0, 32)

	local function AddCounter()
		EggsOpened.Text = (EggAmount)
	end

	local function AddAmount()
		EggAmount = EggAmount + 3
	end

	--Coin Script
	local CoinsBought = Instance.new("ScreenGui")
	local CoinsSpent = Instance.new("TextLabel")
	local CoinSummary = Instance.new("TextLabel")
	local CoinAmount = 0

	--Parents

	CoinsBought.Parent = game.CoreGui
	CoinSummary.Parent = CoinsBought
	CoinsSpent.Parent = CoinsBought


	--ResetOnRespawn

	CoinsBought.ResetOnSpawn = false


	--Active

	CoinSummary.Active = true
	CoinsSpent.Active = true

	--Name

	CoinsBought.Name = ("CoinsBought")
	CoinSummary.Name = ("CoinSummary")
	CoinsSpent.Name = ("CoinsSpent")

	--BackgroundTransparency

	CoinSummary.BackgroundTransparency = (1)
	CoinsSpent.BackgroundTransparency = (1)

	--Selectable

	CoinsSpent.Selectable = false
	CoinSummary.Selectable = false

	--Visible

	CoinsSpent.Visible = true
	CoinSummary.Visible = true

	--Fonts

	CoinsSpent.Font = ("PatrickHand")
	CoinSummary.Font = ("PatrickHand")

	--Text
	CoinSummary.Text = ("You have spent the following amount of coins:")

	--TextColor
	CoinsSpent.TextColor3 = Color3.fromRGB(255, 238, 0)
	CoinSummary.TextColor3 = Color3.fromRGB(255, 238, 0)

	--TextScaled
	CoinSummary.TextScaled = false
	CoinsSpent.TextScaled = true

	--TextSize
	CoinsSpent.TextSize = (16)
	CoinSummary.TextSize = (16)

	--Text Label Positions
	CoinsSpent.Position = UDim2.new(0.705, 0, 0.634, 0)
	CoinSummary.Position = UDim2.new(0.779, 0, 0.554, 0)

	--Text Label Size
	CoinsSpent.Size = UDim2.new(0, 356, 0, 37)
	CoinSummary.Size = UDim2.new(0, 166, 0, 32)

	local function AddCoinsSpent()
		CoinsSpent.Text = (CoinAmount)
	end

	local function AddCoinAmount()
		CoinAmount = CoinAmount + CounterEggCost.Text
	end

	local CounterEggText = CounterEggName.Text
	while UltraEggHatchValue == 1 do
		game.ReplicatedStorage.NetworkRemoteEvent:FireServer("PurchaseEgg", CounterEggText, "Multi")
		--[[game.ReplicatedStorage.NetworkRemoteEvent:FireServer("PurchaseEgg", CounterEggText, "Multi")
		game.ReplicatedStorage.NetworkRemoteEvent:FireServer("PurchaseEgg", CounterEggText, "Multi")
		game.ReplicatedStorage.NetworkRemoteEvent:FireServer("PurchaseEgg", CounterEggText, "Multi")
		game.ReplicatedStorage.NetworkRemoteEvent:FireServer("PurchaseEgg", CounterEggText, "Multi")
		game.ReplicatedStorage.NetworkRemoteEvent:FireServer("PurchaseEgg", CounterEggText, "Multi")
		game.ReplicatedStorage.NetworkRemoteEvent:FireServer("PurchaseEgg", CounterEggText, "Multi")
