--Function Scripts

function OpenGameMenu()
	MainGUI.Visible = true
	CloseMenu.Visible = true
	OpenMenu.Visible = false
end

function OpenUltraMenu()
	UltraFrame.Visible = true
	CloseUltra.Visible = true
	MainGUI.Visible = false
	CloseMenu.Visible = false
end

function CloseUltraMenu()
	UltraFrame.Visible = false
	CloseUltra.Visible = false
	MainGUI.Visible = true
	CloseMenu.Visible = true
end

function CloseMainGUIMenu()
	MainGUI.Visible = false
	CloseMenu.Visible = false
	OpenMenu.Visible = true
end

function OpenUltraCounterMenu()
	UltraCounterFrame.Visible = true
	MainGUI.Visible = false
	CloseMenu.Visible = false
end

function CloseUltraCounterMenu()
	UltraCounterFrame.Visible = false
	CloseMenu.Visible = true
	MainGUI.Visible = true
end

function OpenChangelog()
	MainGUI.Visible = false
	CloseMenu.Visible = false
	ChangelogFrame.Visible = true
end

function CloseTheChangelogMenu()
	ChangelogFrame.Visible = false
	MainGUI.Visible = true
	CloseMenu.Visible = true
end

function OpenBlowBubblesMenu()
	MainGUI.Visible = false
	BlowBubblesFrame.Visible = true
	CloseMenu.Visible = false
end

function CloseBlowBubblesMenu()
	MainGUI.Visible = true
	CloseMenu.Visible = true
	BlowBubblesFrame.Visible = false
end

function StartBlowBubblesAuto()
	BlowBubblesValue = 1
	while wait() do
		while BlowBubblesValue == 1 do
			game.ReplicatedStorage.NetworkRemoteEvent:FireServer("BlowBubble")
			wait()
		end
	end
end

function StopBlowBubblesAuto()
	BlowBubblesValue = 0
end

function OpenProtectionsMenu()
	ProtectionsFrame.Visible = true
	MainGUI.Visible = false
	CloseMenu.Visible = false
end

function CloseProtectionsMenu()
	ProtectionsFrame.Visible = false
	MainGUI.Visible = true
	CloseMenu.Visible = true
end

function OpenAntiAFKMenu()
	ProtectionsFrame.Visible = false
	AntiAFKProtectionMenuFrame.Visible = true
end

function CloseAntiAFKMenuFunction()
	AntiAFKProtectionMenuFrame.Visible = false
	ProtectionsFrame.Visible = true
end

function StartAntiAFK()
	local vu = game:GetService("VirtualUser")
	game:GetService("Players").LocalPlayer.Idled:connect(function()
		vu:Button2Down(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
		wait(1)
		vu:Button2Up(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
	end)
end

