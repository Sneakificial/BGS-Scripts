--ExecutorVersionTextCheck
local BuyService = game:GetService("MarketplaceService")
if game.Players.LocalPlayer.UserId == 145591207 or BuyService:UserOwnsGamePassAsync(game.Players.LocalPlayer.UserId, 15441398) == true then
	wait(3)
	BuiltInExecutorVersion.Text = ("VIP Version")
	BuiltInExecutorVersion.TextColor3 = Color3.fromRGB(251, 255, 0)
else
	if game.Players.LocalPlayer.UserId == 421019467 or game.Players.LocalPlayer.UserId == 2220541483 then
		wait(3)
		BuiltInExecutorVersion.Text = ("Friend VIP Version")
		BuiltInExecutorVersion.TextColor3 = Color3.fromRGB(0, 255, 0)
	end
end

